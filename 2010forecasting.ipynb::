        "# Import packages\n",
        "from scipy.stats import boxcox\n",
        "\n",
        "# Correct path to the sf_bc.csv file\n",
        "file_path = '/content/takehomes/capgemini_3/sf_bc.csv'\n",
        "\n",
        "# Read the data using the corrected file path\n",
        "sf_bc = pd.read_csv(file_path, index_col=0)\n",
        "\n",
        "# Make the target variance stationary\n",
        "sf_bc['weekly_sales_Boxcox'], lam = boxcox(sf_bc['weekly_sales'])\n",
        "\n",
        "# Define the plot_weekly_sales function if it doesn't exist\n",
        "def plot_weekly_sales(sf_bc, y):\n",
        "    \"\"\"Plots weekly sales data.\n",
