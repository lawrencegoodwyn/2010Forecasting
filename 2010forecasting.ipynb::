      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from scipy.stats import boxcox\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "\n",
        "# Load the data\n",
        "file_path = '/content/takehomes/capgemini_3/sf_bc.csv'\n",
        "sf_bc = pd.read_csv(file_path, index_col=0)\n",
        "\n",
        "# 1. Histogram of Original Data\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.histplot(sf_bc['weekly_sales'], kde=True)\n",
        "plt.title('Distribution of Original Weekly Sales')\n",
        "plt.xlabel('Weekly Sales')\n",
        "plt.ylabel('Frequency')\n",
        "plt.show()\n",
        "\n",
        "# Apply Box-Cox transformation\n",
        "sf_bc['weekly_sales_Boxcox'], lam = boxcox(sf_bc['weekly_sales'])\n",
        "\n",
        "# 2. Histogram of Transformed Data\n",
        "plt.figure(figsize=(10, 6))\n",
        "sns.histplot(sf_bc['weekly_sales_Boxcox'], kde=True)\n",
        "plt.title('Distribution of Transformed Weekly Sales (Box-Cox)')\n",
        "plt.xlabel('Transformed Weekly Sales')\n",
        "plt.ylabel('Frequency')\n",
        "plt.show()\n",
        "\n",
        "print(\"Lambda value:\", lam)"
      ],
      "metadata": {
        "id": "smJHwIL1Uota",
        "outputId": "6bb51719-a3bc-4577-9a29-cf5c1388e6de",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 1000
        }
      },
      "id": "smJHwIL1Uota",
      "execution_count": 32,
      "outputs": [
